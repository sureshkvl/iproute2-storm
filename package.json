{
  "name": "netstats-storm",
  "description": "netstats-storm plugin module manages stormflash ",
  "version": "0.0.1",
  "author": {
    "name": "Suresh Kumar"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sureshkvl/netstats-storm"
  },
  "dependencies": {
    "async": ">=0.9.0",
    "fmerge": "1.1.0",
    "mkdirp": ">=0.5.0",
    "stormdata": ">=1.2.0",
    "stormregistry": "1.2.x",
    "stormservice": ">=1.0.2"
  },
